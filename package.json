{
  "name": "yyui-spinner",
  "version": "1.0.0",
  "private": true,
  "description": "yyui-spinner is loading component.",
  "main": "dist/yyui.spinner.js",
  "files": [
    "src",
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "start": "npm run build:umd:min",
    "build:esm": "rollup -c scripts/build.js --environment TARGET:esm",
    "build:umd": "rollup -c scripts/build.js --environment TARGET:umd",
    "build:esm:min": "rollup -c scripts/build.js --environment TARGET:esm-min",
    "build:umd:min": "rollup -c scripts/build.js --environment TARGET:umd-min"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zmkn/yyui-spinner.git"
  },
  "keywords": [
    "yyui",
    "spinner",
    "yyui-spinner"
  ],
  "author": "HZ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zmkn/yyui-spinner/issues"
  },
  "homepage": "https://github.com/zmkn/yyui-spinner#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/plugin-syntax-bigint": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-url": "^6.0.0",
    "@rollup/pluginutils": "^4.1.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.16.0",
    "eslint-plugin-prettier": "^3.3.0",
    "prettier": "^2.2.1",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2"
  }
}